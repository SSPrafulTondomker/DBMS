<% include Header %>


<!-- Product Filer -->
<div class="container-fluid">
	<div class="col-lg-5 col-sm-5 ">
		<div class="ui piled segment">
			<h4 style="text-align: center;"> 
				Wholesaler Finder
			</h4>
			<form action="/Goods" method="POST">
				<div class="ui form" >
				  <div class="two fields">
				    <div class="field ">
				      <label>First Name</label>
				      <input placeholder="First Name" type="text" name= "wsName">
				    </div>
				    <div class="field">
				      <label>Last Name</label>
				      <input placeholder="Last Name" type="text" name= "wslName">
				    </div>
				  </div>
				  <div class="field ">
				    <label>City</label>
				      <input placeholder="City" type="text" name= "wscName">
				  </div>
				  <div class="field ">
				    <label>State</label>
				      <input placeholder="State" type="text" name= "wssName">
				  </div>
				  <div class="ui buttons ">
	  				<button class="ui button" >Cancel</button>
	  				<div class="or"></div>
	  				<button class="ui positive button" type="submit">Save</button>
				   </div>
				</div>
		</div>
	</div>


	
	</form>

	

	<div class="col-lg-7 col-sm-7 ">
		<div class="ui piled segment" >
			<div class="row"></div>
			<div class="container-fluid">
				
				<div class="ui segments col-lg-12">
				<% Goods.forEach(function(s1) 
				{%>
				  <div class="ui green segment">
				    <p><%=s1.userName%></p>
				  </div>
				 
				<% }); %>
				</div>
			</div>
		</div>
	</div>
</div>

<div class="container" style="margin-top: 80px">
	<table class="table table-hover">
  <thead>
    <tr>
      <th scope="col">#</th>
  
      <th scope="col">Name</th>
      <th scope="col">Mobile No.</th>
      <th scope="col">Address </th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <th scope="row">1</th>
      <td>Mark</td>
      <td>9898989898</td>
      <td> delhi </td>
    </tr>
    <tr>
      <th scope="row">2</th>
      <td>Jacob</td>
      <td>849238498</td>
      <td>Hyderabad</td>
    </tr>
    <tr>
      <th scope="row">3</th>
      <td>Larry the Bird</td>
      <td>0923578435</td>
      <td>Chennai</td>
    </tr>
  </tbody>
</table>

<div class="row" style="margin-top: 10% "></div>
<div class="ui divider horizontal green">
	Sale For Farmer
</div>

<div class="container-fluid" style="margin-top: 80px">
	<form action="/dump" method="POST">
	<div class="ui large form">
  <div class="two fields">
    <div class="field">
      <label>UserName</label>
      <input placeholder="UserName" type="text" name="wuName">
    </div>
    <div class="field">
      <label>Product Name</label>
      <input placeholder="Product Name" type="text" name="wpName">
    </div>
    <div class="field ">
      <label>Product Weight</label>
      <input placeholder="Product Weight" type="text" name="wpWeight">
    </div>
  </div>
  <button class="ui col-lg-2 green basic submit button" >Submit</button>
  
</div>
</form>
</div>

<div class="row" style="margin-top: 10% "></div>
<div class="ui divider horizontal green">
	Sale For Retailer
</div>

<div class="container-fluid" style="margin-top: 80px">
	<form action="/dump" method="POST">
	<div class="ui large form">
  <div class="two fields">
    <div class="field">
      <label>UserName</label>
      <input placeholder="UserName" type="text" name="wuName">
    </div>
    <div class="field">
      <label>Product Name</label>
      <input placeholder="Product Name" type="text" name="wpName">
    </div>
    <div class="field ">
      <label>Product Weight</label>
      <input placeholder="Product Weight" type="text" name="wpWeight">
    </div>
  </div>

  <button class="ui col-lg-2 green basic submit button" >Submit</button>
  
</div>
</form>
</div>

<% include Footer %>